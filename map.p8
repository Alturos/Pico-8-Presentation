end

function _draw()
	cls(12)
	-- draws a section of the map
	-- start from location (0,0), 
	-- we want to start drawing at
	-- (0,28) on the screen and we
	-- want to draw 16x16 tiles of
	-- the map.
	map(0,0,0,28,16,16)
	spr(0, 60, 60, 1, 1, left)
	print("map", 116, 122, 7)
end

